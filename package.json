{
    "name": "lms-scraper",
    "version": "1.0.0",
    "description": "Discord bot that scraps the LMS website for notifications",
    "main": "./build/src/index.js",
    "repository": "https://github.com/apteryxxyz/lms",
    "author": "ApteryxXYZ",
    "license": "CC-BY-NC-ND-4.0",
    "private": true,
    "packageManager": "yarn@3.2.1",
    "scripts": {
        "lint": "eslint {src,lib} --fix",
        "format": "prettier src lib *.json --write",
        "build": "rimraf build && tsc && tsc-alias",
        "dev": "ts-node -r tsconfig-paths/register src/index.ts",
        "start": "node ."
    },
    "dependencies": {
        "@apteryxxyz/html-translator": "^1.2.2",
        "@discordjs/builders": "^1.2.0",
        "@maclary/context": "^0.1.2",
        "@wakeful-cloud/html-translator": "^1.1.2",
        "cat-loggr": "^1.2.2",
        "cheerio": "0.22.0",
        "discord.js": "^14.3.0",
        "dotenv": "^16.0.1",
        "fs-extra": "^10.1.0",
        "fuse.js": "^6.6.2",
        "maclary": "^0.4.3",
        "moment": "^2.29.4",
        "node-schedule": "^2.1.0",
        "puppeteer": "^14.1.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.31",
        "@types/fs-extra": "^9.0.13",
        "@types/node-schedule": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "eslint": "^8.15.0",
        "eslint-config-apteryx": "^1.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "prettier-config-apteryx": "^1.0.0",
        "rimraf": "^3.0.2",
        "tsc-alias": "^1.6.7",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.6.4"
    },
    "prettier": "prettier-config-apteryx",
    "eslintConfig": {
        "plugins": [
            "prettier"
        ],
        "extends": [
            "plugin:prettier/recommended",
            "apteryx",
            "apteryx/typescript"
        ]
    }
}
